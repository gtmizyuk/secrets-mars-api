{ 
  "version" : 2, 
  "builds" : [ 
    { 
      "src" : "server.js" , 
      "use" : "@vercel/node" , 
      "config" : { 
        "includeFiles" : [ "db.json" ] 
      } 
    }
  ], 
  "routes" : [ 
    { 
      "src" : "/(.*)" , 
      "dest" : "server.js"
    },
    {
      "src": "/secrets",
      "status": 308,
      "headers": { "Location": "./secrets/index.html" }
    }    
  ] 
}