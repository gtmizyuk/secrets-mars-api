{ 
  "version" : 2, 
  "builds" : [ 
    { 
      "src" : "server.js" , 
      "use" : "@vercel/node" , 
      "config" : { 
        "includeFiles" : [ "db.json" ] 
      } 
    }
  ], 
  "routes" : [ 
    { 
      "src" : "/(.*)" , 
      "dest" : "server.js"
    },
    {
      "src": "/secrets",
      "headers": { "cache-control": "s-maxage=1000" },
      "dest": "/index.html"
    }    
  ] 
}